# Qwen3 Embeddings Server Configuration
# Copy this file to .env and customize as needed

# Model Configuration
MODEL_NAME=mlx-community/Qwen3-Embedding-0.6B-4bit-DWQ

# Server Configuration
HOST=0.0.0.0
PORT=8000

# Performance Settings
MAX_BATCH_SIZE=32
MAX_TEXT_LENGTH=8192

# Logging
LOG_LEVEL=INFO

# CORS Settings
ENABLE_CORS=true
CORS_ORIGINS=*

# Development
DEV_MODE=false